    "guest = int(input())\n",
    "\n",
    "for i in range(guest):\n",
    "    h, w, n = map(int, input().split())\n",
    "    if n % h == 0:\n",
    "        floor = h\n",
    "    else:\n",
    "        floor = n % h\n",
    "    if n / h == n // h:\n",
    "        room = n // h\n",
    "    else:\n",
    "        room = round(n / h + 0.5)\n",
    "    \n",
    "    print(floor * 100 + room)"
